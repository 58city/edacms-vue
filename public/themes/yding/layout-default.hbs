<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<title>{{#if title}}{{title}} - {{/if}}{{#if category}}{{category.name}} - {{/if}}{{siteInfo.title}}</title>
	<link href="/themes/{{siteInfo.theme}}/css/whir_common.css" rel="stylesheet" type="text/css" />
	{{{block "style"}}}
	<script src="/themes/{{siteInfo.theme}}/script/jquery.min.js" type="text/javascript"></script>
	<script src="/themes/{{siteInfo.theme}}/script/jquery.SuperSlide.2.1.1.js" type="text/javascript"></script>
	<!--[if IE 6]>
	<script type="text/javascript" src="/themes/{{siteInfo.theme}}/script/iepng.js"></script>
	<script type="text/javascript"> 
	EvPNG.fix('img,.content,.svc-payment,.svc-gathering,.svc-weg,.svc-tx,.svc-credit,.svc-aa,.svc-donate,.svc-mobile,.svc-escore,.svc-rent,.svc-cashgift,.con,.aoff,'); </script>
	<![endif]-->
</head>
<body class="body">
	{{> header}}
	{{{body}}}
	{{> footer}}
	<!--浮动导航-->
	<div id="xixi" onmouseover="toBig()" onmouseout="toSmall()">
		<table style="float: left" border="0" cellSpacing="0" cellPadding="0" width="57">
			<tr>
				<td>
					<div class="to_top">
						<div class="tt_hd"><a href="#" class="wcup" target="_blank"><img src="/themes/{{siteInfo.theme}}/images/nav1.jpg"></a></div>
						<div class="tt_cont">
							<a href="#" class="mt0" target="_blank">用户</a>
							<a href="#" class="mt1" target="_blank">班级</a>
							<a href="#" class="mt2" target="_blank">课程</a>
							<a href="#" class="mt3" target="_blank">收藏</a>
							<a href="#" class="mt4" target="_blank">记录</a>
							<a href="#" class="mt5" target="_blank">通知</a>
							<a href="#" class="mt6" target="_blank">咨询</a>
							<a id="scrollto" href="#"><img src="/themes/{{siteInfo.theme}}/images/topn.jpg" /></a> </div>
					</div>
					<script type="text/javascript">
						window.onscroll = function () {
							if (document.documentElement.scrollTop + document.body.scrollTop > 500) {
								document.getElementById("scrollto").style.display = "block";
							}
							else {
								document.getElementById("scrollto").style.display = "none";
							}
						}
					</script>
				</td>
			</tr>
		</table>
		<div class="Obtn"></div>
	</div>
	<script src="/themes/{{siteInfo.theme}}/script/index_left_nav.js" type="text/javascript"></script>
	<!--浮动导航-->
</body>
</html>